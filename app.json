{
  "expo": {
    "name": "maiseduca",
    "slug": "maiseduca",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/iconteste.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/educacao.png",
      "resizeMode": "contain",
      "backgroundColor": "#4263EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mirante.maiseduca",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.mirante.maiseduca"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
